<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.String?>
<VBox fx:id="root" spacing="5" style="-fx-padding: 10px;" xmlns="http://javafx.com/javafx/11.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.faforever.moderatorclient.ui.main_window.SettingsController">
    <Separator minHeight="5"/>

    <ChoiceBox fx:id="themeChoiceBox" onAction="#onThemeChoiceChanged">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Default Theme" />
                <String fx:value="Dark Theme" />
            </FXCollections>
        </items>
    </ChoiceBox>
</VBox>
